<div class="container">
  <div class="col-sm-6">
    <img src="images/fruit.jpg" alt="">
  </div>
  <div class="col-sm-6">
    <h1 class="title">Learn to cook cutting edge food</h1>
    <h1>Our Courses</h1>
    <% offeredCourses.forEach(course => { %>
    <h5>
      <%= course.title %>
    </h5>
    <span>$
      <%= course.cost %> </span>
    <% }); %>
  </div>
</div>

